{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "group",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "pattern": "^[A-Za-z0-9_ -]{1,40}$"
    },
    "avatarURL":{
      "type": "string"
    },
    "email":{
      "type": "string"
    },
    "questions": {
      "type": "string"
    },
    "answers": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "name",
    "avatarURL",
    "email",
    "questions"
  ],
  "additionalProperties": false
}
